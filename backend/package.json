{
  "name": "crea-discord-bot-backend",
  "version": "1.0.0",
  "description": "discord bot with various features",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "NODE_ENV=development nodemon app.js",
    "test": "clear && NODE_ENV=test jest --verbose --runInBand --forceExit --watchAll",
    "test:debug": "clear && NODE_ENV=test:debug jest --verbose --runInBand --forceExit",
    "test:coverage": "NODE_ENV=test jest --verbose --runInBand --forceExit --coverage",
    "test:deployment": "NODE_ENV=test jest --verbose --runInBand --forceExit",
    "update:commands": "node ./utils/updateCommands.js",
    "reset": "node ./utils/removeCommands.js",
    "lint": "eslint"
  },
  "author": "Olli Varila",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.1.3",
    "cron": "^2.1.0",
    "discord-interactions": "^3.2.0",
    "discord.js": "^14.6.0",
    "dotenv": "^16.0.3",
    "express": "^4.17.3",
    "mongoose": "^6.7.2"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.19.6",
    "@types/jest": "^29.2.3",
    "axios-mock-adapter": "^1.21.2",
    "eslint": "^8.27.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "jest": "^29.3.1",
    "nodemon": "^2.0.20",
    "supertest": "^6.3.1"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "engines": {
    "node": "19.0.0"
  }
}
